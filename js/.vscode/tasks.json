{
    "version": "0.1.0",
    "command": "wscript",
    "isShellCommand": true,
    

    "tasks": [
        {
            "taskName": "runFile",
            "suppressTaskName": true,
            "showOutput": "always",
            "problemMatcher": "$jshint",
            "args": ["${file}"]
        }
    ]
}